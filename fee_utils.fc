(int) swap_to_ext_chain_fee (int msg_value)  {
    int flat_fee = 5000000000; ;; 5Grams
    int percent_fee = 1500000; ;; 0.15%
    if(msg_value < flat_fee) {
      return msg_value;
    }
    return flat_fee + (msg_value - flat_fee) * percent_fee / 1000000000;
}
(int) swap_to_ton_fee (int total_value) {
    return swap_to_ext_chain_fee(total_value);
}

